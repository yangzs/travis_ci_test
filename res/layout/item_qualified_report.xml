<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/margin_10dim"
    android:paddingLeft="@dimen/margin_25dim"
    android:paddingRight="@dimen/margin_25dim"
    android:paddingTop="@dimen/margin_10dim" >

    <TextView
        android:id="@+id/report_name_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textColor="@color/black_text1"
        android:textSize="@dimen/text_big" />

    <TextView
        android:id="@+id/report_status_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:text="合格"
        android:textColor="@color/black_text2"
        android:textSize="@dimen/text_big" />

</RelativeLayout>